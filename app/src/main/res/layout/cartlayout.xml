<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_margin="10dp"
    android:padding="10dp"
    android:background="@drawable/shapetext"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/txtnamecart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="183dp"
        android:layout_y="9dp"
        android:text="TextView"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="18dp" />

    <TextView
        android:id="@+id/txttotalcart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="133dp"
        android:layout_y="110dp"
        android:text="TextView"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="18dp" />

    <TextView
        android:id="@+id/txtpricecart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="265dp"
        android:layout_y="63dp"
        android:text="TextView"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="18dp" />

    <TextView
        android:id="@+id/txtqtycart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="132dp"
        android:layout_y="65dp"
        android:text="TextView"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="18dp" />

    <ImageView
        android:id="@+id/imgshopp"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_x="10dp"
        android:layout_y="23dp"
        app:srcCompat="@drawable/logo"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="15dp" />

    <ImageView
        android:id="@+id/imgdel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="320dp"
        android:layout_y="106dp"
        app:srcCompat="@android:drawable/ic_delete"
        tools:layout_editor_absoluteX="274dp"
        tools:layout_editor_absoluteY="103dp" />
</AbsoluteLayout>